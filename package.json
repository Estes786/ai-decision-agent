{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "A portfolio website responsive menggunakan Tailwind CSS.",
  "main": "index.js",
  "scripts": {
    "dev": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "serve": "python3 -m http.server 8000",
    "start": "npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "portfolio",
    "tailwindcss",
    "responsive",
    "termux"
  ],
  "author": "Portfolio Owner",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.4.4"
  },
  "dependencies": {
    "strip-bom": "^5.0.0"
  }
}

